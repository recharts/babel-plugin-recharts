{
  "name": "babel-plugin-recharts",
  "version": "2.0.0",
  "description": "Modular Recharts builds without the hassle.",
  "main": "lib/index.js",
  "scripts": {
    "build": "rm -rf lib && babel src --out-dir lib",
    "dev": "rm -rf lib && babel src --out-dir lib -w",
    "test": "mocha --check-leaks --require @babel/register"
  },
  "keywords": [
    "babel-plugin",
    "recharts",
    "modules"
  ],
  "author": "recharts group",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-stage-0": "^7.8.3",
    "@babel/register": "^7.13.8",
    "babel-plugin-add-module-exports": "^1.0.4",
    "mocha": "^10.2.0",
    "recharts": "^2.0.8"
  },
  "files": [
    "lib",
    "*.md"
  ],
  "dependencies": {
    "@babel/parser": "^7.13.10",
    "@babel/traverse": "^7.13.0",
    "@babel/types": "^7.13.0"
  }
}
